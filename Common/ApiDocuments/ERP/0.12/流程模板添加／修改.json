{
        "input": {
                "method": "post",
                "url": "{{pre.erp_url}}",
                "rest": "/erp/flow/save",
                "headers": {
                        "token": "",
                        "_page": "",
                        "_page_size": ""
                },
                "param": [
                        {
                                "name": "id",
                                "in": "formData",
                                "description": "数据id,不传时视为添加操作",
                                "type": "integer"
                        },
                        {
                                "name": "title",
                                "in": "formData",
                                "description": "名称",
                                "required": "true",
                                "type": "string"
                        },
                        {
                                "name": "type",
                                "in": "formData",
                                "description": "合同类型",
                                "required": "true",
                                "type": "integer"
                        },
                        {
                                "name": "notice_type",
                                "in": "formData",
                                "description": "通知类型",
                                "required": "true",
                                "type": "integer"
                        },
                        {
                                "name": "approval_type",
                                "in": "formData",
                                "description": "审批去重类型",
                                "required": "true",
                                "type": "integer"
                        },
                        {
                                "name": "flow",
                                "description": "审批流程(二维数组)",
                                "required": "true",
                                "type": "array",
                                "items": {
                                        "properties": {
                                                "node": {
