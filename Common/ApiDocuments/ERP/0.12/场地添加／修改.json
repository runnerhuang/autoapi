{
        "input": {
                "method": "post",
                "url": "{{pre.erp_url}}",
                "rest": "/erp/space/save",
                "headers": {},
                "param": [
                        {
                                "name": "id",
                                "in": "formData",
                                "description": "数据id,不传时视为添加操作",
                                "type": "integer"
                        },
                        {
                                "name": "name",
                                "in": "formData",
                                "description": "商家名称",
                                "required": "true",
                                "type": "string"
                        },
                        {
                                "name": "code",
                                "in": "formData",
                                "description": "铺位编号",
                                "required": "true",
                                "type": "string"
                        },
                        {
                                "name": "floor_id",
                                "in": "formData",
                                "description": "所属楼层id",
                                "required": "true",
                                "type": "integer"
                        },
                        {
                                "name": "length",
                                "in": "formData",
                                "description": "长度（m）",
                                "required": "true",
                                "type": "number",
                                "format": "double"
                        },
                        {
                                "name": "width",
                                "in": "formData",
                                "description": "宽（m）",
                                "required": "true",
                                "type": "number",
                                "format": "double"
                        },
                        {
                                "name": "contract_id",
                                "in": "formData",
                                "description": "关联合同id",
                                "type": "integer"
                        },
                        {
                                "name": "intro",
                                "in": "formData",
                                "description": "简介",
                                "type": "string"
                        },
                        {
                                "name": "position[0]",
                                "in": "formData",
                                "description": "热点坐标：[x,y]中的x",
                                "type": "integer"
                        },
                        {
                                "name": "position[1]",
                                "in": "formData",
                                "description": "热点坐标：[x,y]中的y",
                                "type": "integer"
                        },
                        {
                                "name": "file_attachment",
                                "in": "formData",
                                "description": "附件的md5字符串，多文件用英文逗号隔开，如：aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa,bbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbb，具体上传参照 <b style='color:red'>附件上传接口</b>",
                                "type": "string"
                        }
                ]
        },
        "output": {
                "code": 0,
                "message": {
                        "EQ": ""
                },
                "status": true,
                "data": {
                        "type": "dict"
                }
        }
}