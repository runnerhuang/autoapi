{
        "input": {
                "method": "post",
                "url": "{{pre.erp_url}}",
                "rest": "/erp/tpl/save",
                "headers": {},
                "param": [
                        {
                                "name": "id",
                                "in": "formData",
                                "description": "数据id,不传时视为添加操作",
                                "type": "integer"
                        },
                        {
                                "name": "name",
                                "in": "formData",
                                "description": "名称",
                                "required": "true",
                                "type": "string"
                        },
                        {
                                "name": "status",
                                "in": "formData",
                                "description": "状态（1正式版本，2草稿）",
                                "required": "true",
                                "type": "integer",
                                "default": 1,
                                "enum": [
                                        1,
                                        2
                                ]
                        },
                        {
                                "name": "type",
                                "in": "formData",
                                "description": "模版类型：0自定义，1铺位",
                                "required": "true",
                                "type": "integer",
                                "default": 1,
                                "enum": [
                                        0,
                                        1
                                ]
                        },
                        {
                                "name": "node",
                                "in": "formData",
                                "description": "自定义模块及元素的json字符串，node里面数据由前端决定",
                                "type": "strint"
                        }
                ]
        },
        "output": {
                "code": 0,
                "message": {
                        "EQ": ""
                },
                "status": true,
                "data": {
                        "type": "dict"
                }
        }
}