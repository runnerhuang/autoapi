{
	"input": {
		"method": "POST",
		"url": "{{pre.eatojoy_url}}",
		"rest": "/user/cart/update",
		"headers": {
			"Content-Type": "application/json",
            "token":"{{pre.eatojoy_user_token}}"
		},
		"param": {
			"id": "{{key.id}}",
			"vendor_id": "{{key.vendor_id}}",
			"product_num": 2,
			"package": [{"id":"{{key.tag_id}}","sid":["{{key.sid1}}","{{key.sid2}}"]}]
		}
	},
	"output": {
		"data": {
			"TYPE": "dict"
		},
		"code": {
			"EQ": 0
		},
		"status": {
			"EQ": true
		},
		"message": {
			"EQ": "更新成功 !"
		}
	}
}
