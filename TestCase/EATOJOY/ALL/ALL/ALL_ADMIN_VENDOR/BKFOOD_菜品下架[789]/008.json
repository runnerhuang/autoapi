{
  "input": {
    "method": "POST",
    "url": "{{pre.eatojoy_url}}",
    "rest": "/admin/food/set_status",
    "headers": {
      "Content-Type": "application/json",
               "eatojoy-admin-api-key":"{{pre.eatojoy_bk_token}}"
    },
    "param": {
      "vendor_id": "{{pre.vendor_id}}",
      "ids":"{{key.food_id}}",
      "status":0
      }
    },
    "output": {
      "data": {
        "TYPE": "dict"
      },
      "code": {
        "EQ": 0
      },
      "status": {
        "EQ": true
      },
      "message": {
        "EQ": ""
      }
    }
  }



