{
  "input": {
    "method": "POST",
    "url": "{{pre.eatojoy_url}}",
    "rest": "/admin/food/edit",
    "headers": {
      "Content-Type": "application/json",
      "eatojoy-admin-api-key":"{{pre.eatojoy_bk_token}}"
    },
    "param": {
      "food_id":"{{key.food_id_3}}",
      "vendor_id": "{{pre.vendor_id}}",
      "num": "商品编号1006{{pre.eatojoy_phone}}",
      "type": "{{key.type_id}}",
      "name": "编辑自动化仅套餐选项{{key.type_id}}",
      "description": "编辑自动化测试仅套餐选项的简介",
      "images": "{{pre.eatojoy_pic}}",
      "price": 0,
      "stock":999,
      "is_wine":0,
      "limited_stock":1,
      "sale_date":0,
      "has_package":2
    }
    },

    "output": {
      "data": {
        "TYPE": "dict"
      },
      "code": {
        "EQ": 0
      },
      "status": {
        "EQ": true
      },
      "message": {
        "EQ": ""
      }
    }
}


