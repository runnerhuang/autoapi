{
	"input": {
		"method": "POST",
		"url": "{{pre.eatojoy_ios_url}}",
		"rest": "/vendor/order/refuse",
		"headers": {
			"Content-Type": "application/json",
              "Cookie":"vendor_token={{key.vendor_token}};vendor_id={{key.vendor_id}}"
		},
		"param": {
			"order_id":"{{key.order_id}}",
			"refused_reason":"拒单原因",
            "refused_type":7
		}
	},
	"output": {
		"data": {
			"EQ": true
		},
		"status": {
			"EQ": true
		},
		"message": {
			"EQ": "拒單成功！"
		},
		"code": {
			"EQ": 0
		}
	}
}
