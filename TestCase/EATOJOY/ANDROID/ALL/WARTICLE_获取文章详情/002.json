{
	"input": {
		"method": "GET",
		"url": "{{pre.eatojoy_android_url}}",
		"rest": "/user/article/detail",
		"headers": {
			"Content-Type": "application/json",
            "token":"{{pre.eatojoy_user_token}}"
		},
		"param": {
			"id":"{{key.article_id}}"
		}
	},
	"output": {
		"data": {
			"TYPE": "dict"
		},
		"code": {
			"EQ": 0
		},
		"status": {
			"EQ": true
		},
		"message": {
			"EQ": "success"
		}
	}
}
