{
	"input": {
		"method": "POST",
		"url": "{{pre.eatojoy_url}}",
		"rest": "/user/cart/add",
		"headers": {
			"Content-Type": "application/json",
            "token":"{{pre.eatojoy_user_token}}"
		},
		"param": {
			"product_id": "{{key.food_id}}",
			"vendor_id": "{{pre.vendor_id}}",
			"product_num": 1,"product_msg": "产品备注",
			"package": [{"id":"{{key.tag_id}}","sid":["{{key.sid1}}","{{key.sid2}}"]}]
		}
	},
	"output": {
		"data": {
			"TYPE": "dict"
		},
		"code": {
			"EQ": 0
		},
		"status": {
			"EQ": true
		},
		"message": {
			"EQ": "添加成功 !"
		}
	}
}
